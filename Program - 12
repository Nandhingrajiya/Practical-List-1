// Write C++ Program To Print Multiplication Table of a Number.

#include<iostream>
using namespace std;
main()
{
	int n,i;
	
	cout<<"My Name Is Nand Hingrajiya"<<endl;
	cout<<"En_no   220130318052"<<endl;
	cout<<"Enter the number : ";
	cin>>n;
	
	for(i=1;i<10;i++)
	{
		cout<<n<<" * "<<i<<" = "<<n*i<<endl;
	}
	cout<<endl;

    return 0;
}
